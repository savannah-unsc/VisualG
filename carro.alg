algoritmo "mcqueen"

var
d,v,c,l,s:real
inicio
escreva(" entre com a distancia")
leia(d)
escreva(" entre com o valor da gasosa")
leia(v)
escreva(" entre com o comsumo da gasosa")
leia(c)

  l:=(c*d)
escreva(" de litrão serão nescessários ",l," litros")
  s:=(v*l)
escreva(" o valor serão de ",s," Reais")
fimalgoritmo
